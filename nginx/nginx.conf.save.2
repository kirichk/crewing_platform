upstream crewing {
    server web:8000;
}

server {

    listen 80;

    location / {
	return 301 https://crewing
	proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $host;
        proxy_redirect off;
    }server{
    
    listen 443;

    location / {
	proxy_pass http://crewing;

}
